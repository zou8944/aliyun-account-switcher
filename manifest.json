{
  "manifest_version": 3,
  "name": "阿里云账户切换器",
  "description": "支持切换阿里云账户",
  "version": "1.0",
  "permissions": [
    "cookies",
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "阿里云账户切换器",
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}