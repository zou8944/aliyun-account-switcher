{
  "manifest_version": 3,
  "name": "Aliyun Account Switcher",
  "description": "阿里云账号切换工具，方便您在多个账号之间自如切换",
  "version": "1.0",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "阿里云账号切换工具"
  },
  "background": {
    "service_worker": "service-worker.js"
  }
}